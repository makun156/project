<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mk.mapper.RecordMapper">

    <insert id="recordOperateInfo">
        insert into sys_record_info(username,message,operate_time,ip_addr) value (#{username},#{message},#{operateTime},#{ipAddr})
    </insert>
</mapper>
